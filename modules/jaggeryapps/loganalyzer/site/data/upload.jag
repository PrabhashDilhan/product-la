<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WSO2 Log Analyzer</title>
    <link rel="icon" href="../../images/favicon.png" type="image/x-icon" />    
    <link href="../../css/bootstrap.min.css" rel="stylesheet">
    <link href="../../css/custom.css" rel="stylesheet">
    <link href="../../css/custom-theme.css" rel="stylesheet">
    <link href="../../css/font-awesome.min.css" rel="stylesheet">
    <link href="../../css/dropzone.min.css" rel="stylesheet">
<script>(function(e,t,n){var r=e.querySelectorAll("html")[0];r.className=r.className.replace(/(^|\s)no-js(\s|$)/,"$1js$2")})(document,window,0);</script>
    <% include("../../includes/tenantAware.jag"); %>
</head>

<body>

<div class="container col-lg-12 col-md-12 col-sm-12">

<!-- header -->
<header>
	<% include("../header/mainnav.jag");%>
</header>
<!-- /header -->

<!-- secondary header - app bar -->
	<% include("../header/sourceconfiguratoinnav.jag");%>
<!-- secondary header - app bar -->

<!-- content/body -->
<div class="container-fluid">
    <div class="row" style="padding-top: 100px;">
        <!-- content -->
                    <div class="col-md-4 col-md-offset-1">
                        <form action="/s" id="upload-logfile-form" method="POST" class="dropzone"  enctype="multipart/form-data">
                        </form>
                    </div>
                    <div class="span6">
                        <div class="vertical-devider">
                        </div>
                     </div>
                    <div class="col-md-5 col-md-offset-1">
                        <label class="input-label">Log Group<span class="help-tip glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="Start Position Type choose where Log analyzer starts initially reading files: at the beginning or at the end."></span></label>
                        <div class="input-group">
                          <div class="input-control text">
                            <select class="form-control" id="src-type" name="sourceType">
                              <option value="file">None</option>
                            </select>
                          </div>
                          <div class="input-group-btn">
                            <button class="btn"><i class="fa fa-plus"></i> </button>
                          </div>
                        </div>
                        <br/>
                        <label class="input-label">Log Stream<span class="help-tip glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="Start Position Type choose where Log analyzer starts initially reading files: at the beginning or at the end."></span></label>
                        <div class="input-group">
                          <div class="input-control text">
                              <input type="text" value="Stream name" class="form-control" placeholder="" id="dataset-delimeter" name="delimeter"/>
                          </div>
                          <div class="input-group-btn">
                            <button class="btn"><i class="fa fa-edit"></i> </button>
                          </div>
                        </div>
                        <br/>
                        <label class="input-label">Start Position<span class="help-tip glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="Start Position Type choose where Log analyzer starts initially reading files: at the beginning or at the end."></span></label>

                        <div class="input-control">
                            <select class="form-control" id="src-type" name="sourceType">
                              <option value="file">Begining</option>
                              <option value="hdfs">Ending</option>
                            </select>
                        </div>

                        <label class="input-label">Delimeter<span class="help-tip glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="set the new line delimiter."></span></label>
                        <span class="fld-required">*</span>
                        <div class="input-control text">
                            <input type="text" value="\n" placeholder="" id="dataset-delimeter" name="delimeter"/>
                        </div>

                        <label class="input-label">Tags<span class="help-tip glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="Add any number of arbitrary tags to your event."></span></label>
                        <div class="input-control text">
                            <input type="text" value="1.0.0" placeholder="" id="dataset-tags" name="tags"/>
                        </div>
                    </div>
        <!-- /content -->
    </div>
<!-- /content/body -->
</div>
</div>

    <script src="../../js/jquery-1.11.1.min.js"></script>
    <script src="../../js/bootstrap.min.js"></script>
    <script src="../../js/wso2.ml.util.js"></script>
    <script src="../../js/jquery.form.min.js"></script>
    <script src="../../js/jquery.dataTables.js"></script>
    <script src="../../js/dropzone.min.js"></script>
    <script type="text/javascript">
    var baseUrl = getBaseUrl(window.location.href);
    var myDropzone = new Dropzone("#upload-logfile-form", { url: "/file/post"});
    Dropzone.options.myAwesomeDropzone = {
      paramName: "file", // The name that will be used to transfer the file
      maxFilesize: 2, // MB
      autoProcessQueue : false,
      accept: function(file, done) {
        if (file.name == "justinbieber.jpg") {
          done("Naha, you don't.");
        }
        else { done(); }
      }
    };

    $('#ddd').ajaxForm({
            url: baseUrl + '/api/files/upload',
            success : function(res){
                 window.location = baseUrl + '/ml/site/data/datasets.jag';
            },
            error :  function(res){

            }
        });
    </script>
</body>
</html>
